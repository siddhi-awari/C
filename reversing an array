#include<stdio.h>
int main()
    {
        int i,n,a[100],rev[100],j=0;
        printf("enter n: ");
        scanf("%d", &n);
        for(i=0;i<=n-1;i++)
        {
            printf("enter value:");
            scanf("%d", &a[i]);
        }
        for(i=n-1;i>=0;i--)
        {
            rev[j]=a[i];
            j++;
        }
        for(i=0;i<=n-1;i++)
        {
            printf("%d\t", rev[i]);
        }
        return 0;
    }
